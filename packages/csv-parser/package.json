{
    "name": "@lukaswagner/csv-parser",
    "version": "0.2.5",
    "description": "Multi-threaded CSV parser using web workers.",
    "type": "module",
    "module": "./lib/csv.js",
    "exports": "./lib/csv.js",
    "types": "./lib/types/csv.d.ts",
    "files": [
        "lib",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "vite build && tsc",
        "vite-watch": "vite build --watch",
        "tsc-watch": "tsc --watch",
        "watch": "concurrently npm:vite-watch npm:tsc-watch",
        "prepare": "npm run build",
        "prepack": "node scripts/prepack.js",
        "postpack": "node scripts/postpack.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lukaswagner/csv-parser.git"
    },
    "keywords": [
        "csv",
        "parser",
        "threaded",
        "worker",
        "parallel"
    ],
    "author": "Lukas Wagner",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lukaswagner/csv-parser/issues"
    },
    "homepage": "https://github.com/lukaswagner/csv-parser#readme",
    "engines": {
        "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
    },
    "devDependencies": {
        "concurrently": "^7.1.0",
        "typescript": "^4.6.3",
        "vite": "^2.9.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
